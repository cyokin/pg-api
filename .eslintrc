{
  "extends": [
    "formidable/configurations/es6-node",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "func-style":"off",
    "no-undef":"off",
    "valid-jsdoc":"off",
    "camelcase":"off",
    "no-use-before-define":"off",
    "no-case-declarations":"off",
    "no-magic-numbers": 0,
    "no-invalid-this": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ],
    "consistent-return": "off",
    "max-params": [
      "error",
      4
    ],
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^next"
      }
    ]
  },
  "env": {
    "mocha": true
  }
}
